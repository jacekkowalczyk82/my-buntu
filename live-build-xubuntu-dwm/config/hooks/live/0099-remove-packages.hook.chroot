#!/bin/bash 

sudo apt-get remove -y --purge indicator-application \
    indicator-bluetooth \
    indicator-datetime \
    indicator-keyboard \
    indicator-power \
    indicator-session indicator-sound \
    gnome-bluetooth \
    libgnome-bluetooth13:amd64 \
