<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<item label="Terminal emulator">
			<action name="Execute">
				<execute>x-terminal-emulator</execute>
			</action>
		</item>
		<item label="Terminator">
			<action name="Execute">
				<execute>terminator</execute>
			</action>
		</item>
		<item label="io.elementary.terminal">
			<action name="Execute">
				<execute>io.elementary.terminal</execute>
			</action>
		</item>
		
		<item label="urxvt">
			<action name="Execute">
				<execute>urxvt</execute>
			</action>
		</item>
		<item label="Sakura">
			<action name="Execute">
				<execute>sakura</execute>
			</action>
		</item>
		<separator/>
		<item label="Run">
			<action name="Execute">
				<execute>gmrun</execute>
			</action>
		</item>
		<item label="Dmenu">
			<action name="Execute">
				<execute>~/.config/dmenu/dmenu-bind.sh</execute>
			</action>
		</item>
		
		<separator/>
		<menu id="root-menu-fevourites-tools" label="Favourite Apps">

		<!--
		<item label="Toxic">
			<action name="Execute">
				<execute>x-terminal-emulator -e /opt/toxic/run_toxic.sh</execute>
			</action>
		</item>
		<item label="Firefox Developer Edition">
			<action name="Execute">
				<execute>firefox.sh</execute>
			</action>
		</item>
		
		--> 
		<item label="Web browser">
			<action name="Execute">
				<execute>x-www-browser</execute>
			</action>
		</item>

		<item label="Firefox">
			<action name="Execute">
				<execute>firefox</execute>
			</action>
		</item>
		
		<item label="Falkon">
			<action name="Execute">
				<execute>falkon</execute>
			</action>
		</item>
		<!-- This requires the presence of the 'openbox-menu' package to work -->
		<separator/>
		<item label="File Manager">
			<action name="Execute">
				<execute>io.elementary.files</execute>
			</action>
		</item>
                <item label="Geany">
			<action name="Execute">
				<execute>geany</execute>
			</action>
		</item>
		<item label="io.elementary.code">
			<action name="Execute">
				<execute>io.elementary.code</execute>
			</action>
		</item>
		
		<item label="Meld">
			<action name="Execute">
				<execute>meld</execute>
			</action>
		</item>
		<item label="Audacious">
			<action name="Execute">
				<execute>audacious</execute>
			</action>
		</item>
		<item label="VLC">
			<action name="Execute">
				<execute>vlc</execute>
			</action>
		</item>
		<item label="MPV Media Player">
			<action name="Execute">
				<execute>mpv --player-operation-mode=pseudo-gui --</execute>
			</action>
		</item>
		<item label="io.elementary.music">
			<action name="Execute">
				<execute>io.elementary.music</execute>
			</action>
		</item>
		<item label="io.elementary.videos">
			<action name="Execute">
				<execute>io.elementary.videos</execute>
			</action>
		</item>
		<item label="Htop">
			<action name="Execute">
				<execute>x-terminal-emulator -e htop</execute>
			</action>
		</item>

		</menu>
		<menu id="screenshot-menu-754113" label="Screenshot">
			<item label="Whole screen">
				<action name="Execute">
					<execute>scrot</execute>
				</action>
			</item>
			<item label="Select rectangle">
				<action name="Execute">
					<execute>scrot -s </execute>
				</action>
			</item>
		</menu>
		<separator/>
		<menu id="/Debian"/>
		<separator/>
		<menu execute="/usr/bin/obamenu" id="applications-menu" label="Applications"/>
		<separator/>

		<menu id="root-menu-fopenbox-tools" label="Openbox tools">
		<item label="Obmenu">
			<action name="Execute">
				<execute>obmenu</execute>
			</action>
		</item>
		<item label="ObConf">
			<action name="Execute">
				<execute>obconf</execute>
			</action>
		</item>
		<item label="Reconfigure">
			<action name="Reconfigure"/>
		</item>
		<item label="Restart">
			<action name="Restart"/>
		</item>
		</menu>

		<separator/>
		<item label="Exit Openbox">
			<action name="Exit"/>
		</item>
		<item label="Lock Screen">
			<action name="Execute">
				<execute>i3lock -i /usr/share/backgrounds/elementaryos-default</execute>
			</action>
		</item>

		<menu id="root-menu-814533" label="System">
			<item label="Reboot">
				<action name="Execute">
					<execute>systemctl reboot</execute>
				</action>
			</item>
			<item label="Poweroff">
				<action name="Execute">
					<execute>systemctl poweroff</execute>
				</action>
			</item>
		</menu>
		<separator/>
	</menu>
</openbox_menu>
